@page
@model NachoTacos.Server.Authentication.Web.Pages.Private.MyPersonalPageModel
@{
    ViewData["Title"] = "My Personal Page";
}

<div class="text-center">
    <h1 class="display-4">My Personal Page</h1>
    <p>You've successfully been authenticated!</p>
    <h2>Claims</h2>
    <dl>
        @foreach (var claim in User.Claims)
        {
            <dt>@claim.Type</dt>
            <dd>@claim.Value</dd>
        }
    </dl>
</div>
